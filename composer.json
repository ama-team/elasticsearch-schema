{
  "name": "ama-team/elasticsearch-schema-definitions",
  "description": "Declarative definition of ElasticSearch configuration",
  "license": "MIT",
  "authors": [
    {
      "name": "AMA Team",
      "email": "dev@amagroup.ru"
    }
  ],
  "autoload": {
    "psr-4": {
      "AmaTeam\\ElasticSearch\\Schema\\Definition\\": "src/php/src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AmaTeam\\ElasticSearch\\Schema\\Definition\\Test\\": "src/php/tests"
    }
  },
  "require": {
    "php": ">= 5.5.9",
    "symfony/yaml": ">= 3.1.0, < 5.0.0"
  },
  "require-dev": {
    "codeception/codeception": "^2.3",
    "allure-framework/allure-codeception": "^1.2"
  },
  "config": {
    "vendor-dir": "src/php/vendor",
    "bin-dir": "src/php/bin"
  }
}
